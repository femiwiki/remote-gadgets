{
  "private": true,
  "scripts": {
    "lint-filename": "test -z \"$(yarn run --silent find-filename)\" || (yarn run find-filename && false)",
    "find-filename": "find pages/ gadgets/ -not -regex \".+/\\([a-z\\._/:-]+\\)?\"",
    "lint": "prettier . --check && yarn run lint-filename",
    "fix": "prettier . --write"
  },
  "devDependencies": {
    "prettier": "^2.0.5"
  }
}
